# Ignore directories generated by Composer.
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/config/

# Ignore sensitive information.
/web/sites/*/settings*.php
/web/sites/*/services*.yml

# Ignore Drupal's file directory.
/web/sites/*/files/
/web/sites/files/

# Ignore directories generated by drush.
/drush/
/drush/contrib/

# Ignore directories generated by the installation.
/web/sites/*/private/
/web/sites/*/files/
/web/sites/default/files/

# Ignore IDE specific files.
.idea/
*.swp
*.swo
.DS_Store

# Ignore files generated by macOS.
._*

# Ignore npm dependency directories (if you're using them).
node_modules/

# Ignore configuration export directory.
config/sync/

# Other files typically ignored in a Drupal project.
*.sql
*.tar
*.tar.gz
*.log

# Ignore the Docker files if you don't want them in version control.
# Comment these out if you want to include Docker configuration.
#docker-compose.yml
#Dockerfile

# Add custom paths relative to the .gitignore.
